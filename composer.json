{
  "name": "struggle-for-php/sfp-deptrac-builtin",
  "description": "Deptrac extension to enforce application on framework rule.",
  "keywords": ["deptrac", "header", "static analysis", "static-analysis"],
  "license": "MIT",
  "authors": [
    {
      "name": "sasezaki",
      "email": "sasezaki@gmail.com"
    }
  ],
  "require": {
      "qossmic/deptrac": "0.24",
      "php": "^7.4 || ~8.0.0 || ~8.1.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5",
      "squizlabs/php_codesniffer": "^3.7",
      "laminas/laminas-coding-standard": "^2.4",
    "vimeo/psalm": "^4.27",
    "psalm/plugin-phpunit": "^0.17.0"
  },
  "autoload": {
      "psr-4": {
        "Sfp\\Deptrac\\Builtin\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "SfpTest\\Deptrac\\Builtin\\": "tests"
      }
    },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
          "dealerdirect/phpcodesniffer-composer-installer": true
      },
      "platform": {
        "php": "7.4.99"
    }
  }
}
